{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "strict": 0,
    "arrow-parens": ["error", "always"],
    "comma-dangle": [2, "always-multiline"],
    "import/no-named-as-default": 0,
    "import/prefer-default-export": 0,
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "class-methods-use-this": 0,
    "max-len": [1, { "code": 120 }]
  }
}
