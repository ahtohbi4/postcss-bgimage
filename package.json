{
  "author": "Alexandr Antonov",
  "name": "postcss-bgimage",
  "version": "2.0.0-beta.0",
  "description": "Plugin for PoscCSS to remove or keep only images by url() to optimize page loading",
  "main": "./src/index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "lint:js": "node_modules/.bin/eslint ./src/",
    "lint:staged": "lint-staged",
    "test": "mocha --require babel-polyfill --compilers js:babel-register test/*.js test/**/*.js",
    "test:watch": "mocha --watch --require babel-polyfill --compilers js:babel-register test/*.js test/**/*.js",
    "prepublishOnly": "lint:js && npm test"
  },
  "lint-staged": {
    "*.js": "lint:js"
  },
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahtohbi4/postcss-bgimage.git"
  },
  "keywords": [
    "postcss",
    "postcss-plugin",
    "optimization",
    "background",
    "background-image",
    "css"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahtohbi4/postcss-bgimage/issues"
  },
  "homepage": "https://github.com/ahtohbi4/postcss-bgimage#readme",
  "dependencies": {
    "css-color-list": "0.0.2",
    "postcss": "6.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "expect": "1.20.2",
    "lint-staged": "^4.0.2",
    "mocha": "3.3.0",
    "pre-commit": "^1.2.2"
  },
  "babel": {
    "presets": [
      "babel-preset-es2015",
      "babel-preset-stage-0"
    ]
  }
}
